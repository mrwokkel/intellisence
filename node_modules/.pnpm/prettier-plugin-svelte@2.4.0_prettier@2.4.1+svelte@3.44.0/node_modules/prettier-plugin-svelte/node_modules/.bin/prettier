#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier@2.4.1/node_modules/prettier/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier@2.4.1/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/node_modules:/Users/johnvandijk/Code/2021/gown/node_modules:/Users/johnvandijk/Code/2021/node_modules:/Users/johnvandijk/Code/node_modules:/Users/johnvandijk/node_modules:/Users/node_modules:/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier-plugin-svelte@2.4.0_prettier@2.4.1+svelte@3.44.0/node_modules/prettier-plugin-svelte/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier-plugin-svelte@2.4.0_prettier@2.4.1+svelte@3.44.0/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier@2.4.1/node_modules/prettier/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier@2.4.1/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/node_modules:/Users/johnvandijk/Code/2021/gown/node_modules:/Users/johnvandijk/Code/2021/node_modules:/Users/johnvandijk/Code/node_modules:/Users/johnvandijk/node_modules:/Users/node_modules:/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier-plugin-svelte@2.4.0_prettier@2.4.1+svelte@3.44.0/node_modules/prettier-plugin-svelte/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/prettier-plugin-svelte@2.4.0_prettier@2.4.1+svelte@3.44.0/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../prettier@2.4.1/node_modules/prettier/bin-prettier.js" "$@"
else
  exec node  "$basedir/../../../../../prettier@2.4.1/node_modules/prettier/bin-prettier.js" "$@"
fi
