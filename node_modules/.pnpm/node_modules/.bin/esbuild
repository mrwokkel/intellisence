else
  export NODE_PATH="$NODE_PATH:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/bin/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/esbuild@0.13.8/node_modules/esbuild/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/esbuild@0.13.8/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/node_modules:/Users/johnvandijk/Code/2021/gown/node_modules:/Users/johnvandijk/Code/2021/node_modules:/Users/johnvandijk/Code/node_modules:/Users/johnvandijk/node_modules:/Users/node_modules:/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/node_modules/esbuild/bin/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/node_modules/esbuild/node_modules"
