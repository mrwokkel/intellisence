#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/eslint@7.32.0/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/node_modules:/Users/johnvandijk/Code/2021/gown/node_modules:/Users/johnvandijk/Code/2021/node_modules:/Users/johnvandijk/Code/node_modules:/Users/johnvandijk/node_modules:/Users/node_modules:/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/bin/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/eslint@7.32.0/node_modules/eslint/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/eslint@7.32.0/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules/.pnpm/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/intellisence/node_modules:/Users/johnvandijk/Code/2021/gown/rommel/node_modules:/Users/johnvandijk/Code/2021/gown/node_modules:/Users/johnvandijk/Code/2021/node_modules:/Users/johnvandijk/Code/node_modules:/Users/johnvandijk/node_modules:/Users/node_modules:/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/eslint.js" "$@"
else
  exec node  "$basedir/../../bin/eslint.js" "$@"
fi
